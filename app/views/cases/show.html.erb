<p id="notice"><%= notice %></p>

<div class="container">

<div class="col-lg-12">

<div class="col-md-2">

</div>

<div class="col-md-8">
<h2 class="text-center"><%= @case.sponsor %> Case: <%= @case.title %></h2>
</div>
<div class="col-md-2">

 

<div style="box-shadow: 0px 0px 5px #fff;">
<%= link_to new_feedback_path do |c| %>
<p style="display:inline;">Submit Feedback</p>
<%= fa_icon "arrow-right 2x", class: 'float-right', :style => 'margin-top:30px'%>
<% end %>
</div>



</div>
</div>
<div class="row">
<p>  <%= @case.description %></p>


<h4> Submit Case</h4>
<%= form_for(@case, :html => { :multipart => true }) do |form| %>
  <%= form.file_field :document %>
  <%= form.submit %>
<% end %>
</div>
</div>