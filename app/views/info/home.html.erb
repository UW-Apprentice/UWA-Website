	<body>

    <!-- Header -->
    <header id="top" class="header">
        <div class="text-vertical-center">

        <center><%= image_tag("logo-home.png", class: 'img-responsive', :style => 'width:100px') %></center>
            <h1>Innovate. Impact. Experience.</h1>
            <h3>Apprentice Is The Premier Case Competition For Waterloo's Most Innovative Students</h3>
            <br>

            <%= button_tag "Login", class: 'btn btn-success', :'data-toggle' => 'modal', :'data-target' => '#loginModal'%>

            <ul class="list-inline social" style="padding-top:60px;">
                        <li>
                        <%= link_to "https://www.facebook.com/uwapprentice", method: :put do %>
             			 <%= fa_icon "facebook 2x" %>
              <% end %></li>

              <li><%= link_to "https://twitter.com/UWApprentice", method: :put do %>
              <%= fa_icon "twitter 2x" %>
              <% end %></li>

              <li><%= mail_to "uwaterlooapprentice@gmail.com", method: :put do %>
              <%= fa_icon "envelope 2x" %>
              <% end %></li>
 
                    </ul>
        </div>
    </header>

    <!-- About -->
    <section id="about" class="about">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2>What is Apprentice?</h2>
                    <p class="lead"> Apprentice is a weekend long case competition where students solve real problems for real startups. On March 13-15, four exciting startups from the Waterloo community will present a problem to our cohort of 20 delegates and receive creative solutions to create growth, value and tangible impact. The startups and their cases provide a diverse picture of the startup world. This weekend is your chance to show yourself and the world that you have the grit, intelligence, eloquence, and tools to become Waterlooâ€™s next great entrepreneur.</p>
                </div>
            </div>
            <!-- /.row -->
        </div>
        <!-- /.container -->
    </section>

    <section id="faq" class="services bg-primary">
        <div class="container">

<%= render 'faqs'%>

      </div>
    </section>

    <!-- GBTTT create model -->
    <section id="schedule" class="services">
        <div class="container">
        <div class="col-lg-12 text-center">
        	<h2>Schedule</h2>

        	<div class="col-md-4">

        	<h3>Friday</h3>

          <% Itinerary.where(:day => 'friday').order(item_time: :desc).each do |i| %>

          <h4><%= i.item %></h4>

          <!-- Get time from db format it right and remove leading zero -->
          <h5><%= Time.strptime(i.item_time.to_s(:time), "%H:%M").strftime("%I:%M%P")[1..-1] %>
          - <%= Time.strptime(i.item_time_end.to_s(:time), "%H:%M").strftime("%I:%M%P")[1..-1] %></h5>
          <% end %>

          </div>
        	<div class="col-md-4">
        	<h3>Saturday</h3>

          <% Itinerary.where(:day => 'saturday').order(item_time: :desc).each do |i| %>

          <h4><%= i.item %></h4>

          <!-- Get time from db format it right and remove leading zero -->
          <h5><%= Time.strptime(i.item_time.to_s(:time), "%H:%M").strftime("%I:%M%P")[1..-1] %>
          - <%= Time.strptime(i.item_time_end.to_s(:time), "%H:%M").strftime("%I:%M%P")[1..-1] %></h5>

          <% end %>
</div>
        	<div class="col-md-4">
        	<h3>Sunday</h3>
        	 <% Itinerary.where(:day => 'sunday').order(item_time: :desc).each do |i| %>

          <h4><%= i.item %></h4>

          <!-- Get time from db format it right and remove leading zero -->
          <h5><%= Time.strptime(i.item_time.to_s(:time), "%H:%M").strftime("%I:%M%P")[1..-1] %>
          - <%= Time.strptime(i.item_time_end.to_s(:time), "%H:%M").strftime("%I:%M%P")[1..-1] %></h5>

          <% end %>
</div>

        </div>
        </section>

<section id="team" class="services bg-primary">
        <div class="container">
        <div class="col-lg-12 text-center">
        <h2>Team</h2>

<% Exec.where(:position => 'President').each do |p| %>

        <div class="col-md-4">

        <center><%= image_tag(p.picture, :class => 'img-responsive') %></center>
        <h3><%= p.exec_name %></h3>
        <h4><%= p.position %></h4>
        <h4><%= p.program %></h4>
        </div>
        <% end %>

        </div>
<center><%= button_tag "Full Team", class: 'btn btn-success', :'data-toggle' => 'modal', :'data-target' => '#teamModal'%></center>

        </div>
        </section>

        <section id="sponsors" class="services">
<div class="container">
        <div class="col-lg-12 text-center">

<h2>Sponsors</h2>
<div class="col-lg-12">
<% Case.where(:case_sponsor => false).each do |s| %>

<div class="col-md-4">

<%= image_tag(s.sponsor_logo, :class => 'img-responsive') %>
</div>

<% end %>
</div>

<div class="col-lg-12">
 <h2>Case Hosts</h2>
<% Case.where(:case_sponsor => true).each do |s| %>

<div class="col-md-4">

<%= image_tag(s.sponsor_logo, :class => 'img-responsive') %>
<br>
</div>

<% end %>
</div>
        </div>
        </div>
        </section>



