<%= form_for(@feedback) do |f| %>
  <% if @feedback.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@feedback.errors.count, "error") %> prohibited this feedback from being saved:</h2>

      <ul>
      <% @feedback.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

<!-- NOTE: Labels are correct, but where the points are allocated in the db is wrong.  But delegate still get their points !!!!!!!!!! -->
  <div class="field">
    <%= f.label "Receiving Delegate (One by one for now)" %><br>
    <%= select("feedback", "receiver", Delegate.all.collect {|d|
     [ d.fullname] }, {include_blank: 'None'}, class: "form-control", :required => true)

    %>
  </div>
 <!--
 <div class="field">
    <%= f.label "Case Number" %><br>
    <%= f.number_field :leadership, class: "form-control", in: 1...5, :required => true  %>
  </div>-->
  <div class="field">
    <%= f.label :case_number %><br>
    <%= f.number_field :case_number, class: "form-control", in: 1...5, :required => true  %>
  </div>
  <div class="field">
    <%= f.label :case_position %><br>
    <%= f.number_field :case_position, class: "form-control", in: 1...4, :required => true   %>
  </div>
  <div class="field">
    <%= f.label :exec_secret %><i>&nbsp;Please ensure you enter the correct Key for each of the forms. They are different!!!!</i><br>
    <%= f.number_field :exec_secret, class: "form-control", :required => true  %>
  </div>

  <div class="actions">
    <%= f.submit "Give Feedback", :class => 'btn btn-success'  %>
  </div>
<% end %>
