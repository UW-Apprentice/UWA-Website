<h1>Listing feedbacks</h1>

<table>
  <thead>
    <tr>
      <th>Giver</th>
      <th>Good comments</th>
      <th>Improvement comments</th>
      <th>Leadership</th>
      <th>Creativity</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @feedbacks.each do |feedback| %>
      <tr>
        <td><%= feedback.giver %></td>
        <td><%= feedback.good_comments %></td>
        <td><%= feedback.improvement_comments %></td>
        <td><%= feedback.leadership %></td>
        <td><%= feedback.creativity %></td>
        <td><%= link_to 'Show', feedback %></td>
        <td><%= link_to 'Edit', edit_feedback_path(feedback) %></td>
        <td><%= link_to 'Destroy', feedback, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Feedback', new_feedback_path %>
